var number = prompt("Pick a number between 1 and 10...");

if(number == 7) {
	alert("You got it!");
}

else if(number > 0 && number < 7) {
	alert("Too low.");
}

else if(number > 7 && number < 11) {
	alert("Too high.");
}

else {
	alert("Read the directions, smart guy.")
}
